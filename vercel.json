{
  "redirects": [
    {
      "source": "/:path(.*)/",
      "destination": "/:path",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.vpngid.com" }],
      "destination": "https://vpngid.com/:path*",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/best-vpn.html",
      "destination": "/best-vpn",
      "permanent": true
    },
    {
      "source": "/coupons.html",
      "destination": "/coupons",
      "permanent": true
    },
    {
      "source": "/articles/index.html",
      "destination": "/articles",
      "permanent": true
    },
    {
      "source": "/articles/what-is-vpn.html",
      "destination": "/articles/what-is-vpn",
      "permanent": true
    },
    {
      "source": "/articles/how-to-choose-vpn.html",
      "destination": "/articles/how-to-choose-vpn",
      "permanent": true
    },
    {
      "source": "/articles/are-free-vpn-safe.html",
      "destination": "/articles/are-free-vpn-safe",
      "permanent": true
    },
    {
      "source": "/en/index.html",
      "destination": "/en",
      "permanent": true
    },
    {
      "source": "/en/best-vpn.html",
      "destination": "/en/best-vpn",
      "permanent": true
    },
    {
      "source": "/en/coupons.html",
      "destination": "/en/coupons",
      "permanent": true
    },
    {
      "source": "/en/articles/index.html",
      "destination": "/en/articles",
      "permanent": true
    },
    {
      "source": "/en/articles/what-is-vpn.html",
      "destination": "/en/articles/what-is-vpn",
      "permanent": true
    },
    {
      "source": "/en/articles/how-to-choose-vpn.html",
      "destination": "/en/articles/how-to-choose-vpn",
      "permanent": true
    },
    {
      "source": "/en/articles/are-free-vpn-safe.html",
      "destination": "/en/articles/are-free-vpn-safe",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/img/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
