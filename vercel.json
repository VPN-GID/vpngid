{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/best-vpn.html",
      "destination": "/best-vpn",
      "permanent": true
    },
    {
      "source": "/coupons.html",
      "destination": "/coupons",
      "permanent": true
    },
    {
      "source": "/articles/index.html",
      "destination": "/articles/",
      "permanent": true
    },
    {
      "source": "/articles/what-is-vpn.html",
      "destination": "/articles/what-is-vpn",
      "permanent": true
    },
    {
      "source": "/articles/how-to-choose-vpn.html",
      "destination": "/articles/how-to-choose-vpn",
      "permanent": true
    },
    {
      "source": "/articles/are-free-vpn-safe.html",
      "destination": "/articles/are-free-vpn-safe",
      "permanent": true
    },
    {
      "source": "/en/index.html",
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/en/best-vpn.html",
      "destination": "/en/best-vpn",
      "permanent": true
    },
    {
      "source": "/en/coupons.html",
      "destination": "/en/coupons",
      "permanent": true
    },
    {
      "source": "/en/articles/index.html",
      "destination": "/en/articles/",
      "permanent": true
    },
    {
      "source": "/en/articles/what-is-vpn.html",
      "destination": "/en/articles/what-is-vpn",
      "permanent": true
    },
    {
      "source": "/en/articles/how-to-choose-vpn.html",
      "destination": "/en/articles/how-to-choose-vpn",
      "permanent": true
    },
    {
      "source": "/en/articles/are-free-vpn-safe.html",
      "destination": "/en/articles/are-free-vpn-safe",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/img/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
